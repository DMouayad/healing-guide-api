{
	"name": "healing-guide-api",
	"version": "1.0.14",
	"description": "A Restful API for the HealingGuide app",
	"author": "Mouayad Alhamwi",
	"repository": "DMouayad/healing-guide-api",
	"license": "MIT",
	"main": "index.ts",
	"private": true,
	"scripts": {
		"dev": "tsx watch --clear-screen=false src/index.ts | pino-pretty",
		"build": "tsup",
		"start": "node dist/index.js",
		"clean": "rimraf dist coverage",
		"lint": "biome check src/",
		"lint:fix": "biome check src/ --fix",
		"format": "biome format src/",
		"test": "vitest run",
		"prepare": "husky"
	},
	"dependencies": {
		"@asteasolutions/zod-to-openapi": "^7.1.1",
		"@types/pg": "^8.11.10",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"envalid": "^8.0.0",
		"express": "^5.0.0",
		"express-list-endpoints": "^7.1.0",
		"express-rate-limit": "^7.4.0",
		"helmet": "^7.1.0",
		"http-status-codes": "^2.3.0",
		"kysely": "^0.27.4",
		"libphonenumber-js": "^1.11.9",
		"morgan": "^1.10.0",
		"pg": "^8.13.0",
		"pino": "^9.4.0",
		"swagger-ui-express": "^5.0.1",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.1",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/express-requests-logger": "^4.0.0",
		"@types/morgan": "^1.9.9",
		"@types/supertest": "^6.0.2",
		"@types/swagger-ui-express": "^4.1.6",
		"husky": "^9.1.6",
		"kysely-codegen": "^0.16.8",
		"kysely-ctl": "^0.9.0",
		"lint-staged": "^15.2.10",
		"pino-pretty": "^11.2.2",
		"rimraf": "^6.0.1",
		"supertest": "^7.0.0",
		"tsup": "^8.3.0",
		"tsx": "^4.19.1",
		"typescript": "^5.6.2",
		"vite-tsconfig-paths": "^5.0.1",
		"vitest": "^2.1.1"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": ["biome check --write --no-errors-on-unmatched"]
	},
	"tsup": {
		"entry": ["src", "!src/**/__tests__/**", "!src/**/*.test.*"],
		"splitting": false,
		"sourcemap": true,
		"clean": true
	},
	"packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1"
}
